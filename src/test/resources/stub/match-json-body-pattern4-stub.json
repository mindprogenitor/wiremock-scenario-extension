{
	"request": {
		"customMatcher": {
			"name": "concurrent-session",
			"parameters": {
			    "request": {
			        "method": "POST",
			        "urlPath": "/test",
			        "bodyPatterns": [
			    					 { "equalToJson": "{ \"first\":1,\"second\":2}",
			    					   "ignoreArrayOrder": true } ]
			    }
			}
		}
	},
    "response": {
        "status": 200,
        "body": "MATCHED-LOOSELY",
        "headers": { "Content-Type": "text/text" }
    }
}